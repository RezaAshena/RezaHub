@model RezaHub.ViewModels.UserVM2

@{
    ViewData["Title"] = "List User";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Users</h1>

<table class="table table-bordered table-hover">
    <thead>
        <tr>Users</tr>
    </thead>
    <tbody>
        @foreach (var user in Model.Users)
        {
            <tr>
                <td>@Html.ActionLink(user.Name, "detail", "Users", new { id = user.Id }, null) </td>
            </tr>
        }
    </tbody>

</table>

